   "source": [
    "def maxSum(arr):\n",
    "    \n",
    "    current_sum = max_sum = arr[0]\n",
    "    \n",
    "    for i in arr[1:]:\n",
    "        current_sum = max(current_sum+i,i)\n",
    "        max_sum = max(current_sum,max_sum)\n",
    "    return max_sum"
   ]
