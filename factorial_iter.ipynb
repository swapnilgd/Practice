    "def fact1(n):\n",
    "    \n",
    "    if n == 0:\n",
    "        return 1\n",
    "    factorial = 1\n",
    "    \n",
    "    while n > 1:\n",
    "        factorial = factorial * n\n",
    "        n -= 1\n",
    "    return factorial"
   ]
