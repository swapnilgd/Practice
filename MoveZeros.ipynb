    "def MoveZeros(arr):\n",
    "    \n",
    "    if len(arr) < 2:\n",
    "        return arr\n",
    "    \n",
    "    i = 0\n",
    "    for j in range(len(arr)):\n",
    "        if arr[j] != 0:\n",
    "            arr[i],arr[j] = arr[j],arr[i]\n",
    "            i += 1\n",
    "    print(arr)"
   ]
